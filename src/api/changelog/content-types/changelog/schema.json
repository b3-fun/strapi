{
  "kind": "collectionType",
  "collectionName": "changelogs",
  "info": {
    "singularName": "changelog",
    "pluralName": "changelogs",
    "displayName": "changelog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "visible": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true,
      "minLength": 1
    },
    "date": {
      "type": "datetime",
      "required": true,
      "unique": true
    },
    "media": {
      "allowedTypes": [
        "images",
        "videos"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
